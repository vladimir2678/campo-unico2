<?xml version="1.0" ?>
<openerp>
    <data>

        <record id="crossovered_budget_totals_view_form" model="ir.ui.view">
            <field name="name">crossovered.budget.totals.view.form</field>
            <field name="model">crossovered.budget</field>
            <field name="inherit_id" ref="account_budget.crossovered_budget_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="before">
                    <group>
                        <group name="group_totals1">
                            <field name="amount_planned"/>
                            <field name="amount_practical"/>
                            <field name="amount_practical_perc"/>
                            <field name="forecast"/>
                        </group>
                        <group name="group_totals2">
                        </group>
                    </group>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
